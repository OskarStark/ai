# Multi-Agent Examples Environment Configuration
# Copy this file to .env.local and configure your API keys

# OpenAI Configuration (Required for examples)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Alternative Platform Configurations
# ANTHROPIC_API_KEY=your_anthropic_key_here
# GOOGLE_API_KEY=your_google_key_here
# AZURE_OPENAI_ENDPOINT=your_azure_endpoint_here
# AZURE_OPENAI_API_KEY=your_azure_key_here

# Multi-Agent System Configuration
MULTIAGENT_MAX_ITERATIONS=5
MULTIAGENT_DEFAULT_TEMPERATURE=0.5
MULTIAGENT_RESEARCH_TEMPERATURE=0.1
MULTIAGENT_WRITING_TEMPERATURE=0.7
MULTIAGENT_REVIEW_TEMPERATURE=0.2
MULTIAGENT_ANALYSIS_TEMPERATURE=0.3

# Logging Configuration
LOG_LEVEL=info
VERBOSE_OUTPUT=false